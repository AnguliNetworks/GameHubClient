{
  "extends": "airbnb",
  "env": {
    "browser": true
  },
  "rules": {
    "indent": [
      "error",
      4,
      {
        "SwitchCase": 4
      }
    ],
    "react/jsx-indent": [
      "error",
      4
    ],
    "react/jsx-indent-props": [
      "error",
      4
    ],
    "comma-dangle": [
      "error",
      "never"
    ],
    "padded-blocks": [
      "error",
      "always"
    ]
  }
}
